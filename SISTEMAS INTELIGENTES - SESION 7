        "import numpy as np\n",
        "import networkx as nx\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Generar una red de estructura molecular de un cristal de rubí\n",
        "G = nx.grid_graph(dim=[4, 4, 4])\n",
        "\n",
        "# Graficar la estructura molecular\n",
        "nx.draw(G, node_size=10)\n",
        "plt.title('Estructura molecular de un cristal de rubí')\n",
        "plt.show()\n"
      ]
