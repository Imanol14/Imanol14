      "cell_type": "code",
      "source": [
        "import networkx as nx\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# Creamos un grafo dirigido\n",
        "G = nx.DiGraph()\n",
        "\n",
        "# Definimos las etapas de la evolución humana como nodos\n",
        "etapas = [\"Australopithecus\", \"Homo habilis\", \"Homo erectus\", \"Homo sapiens\",\n",
        "          \"Homo neanderthalensis\", \"Homo floresiensis\", \"Homo luzonensis\"]\n",
        "\n",
        "# Añadimos los nodos al grafo\n",
        "G.add_nodes_from(etapas)\n",
        "\n",
        "# Definimos las relaciones evolutivas como aristas\n",
        "relaciones = [(\"Australopithecus\", \"Homo habilis\"),\n",
        "              (\"Homo habilis\", \"Homo erectus\"),\n",
        "              (\"Homo erectus\", \"Homo sapiens\"),\n",
        "              (\"Homo sapiens\", \"Homo neanderthalensis\"),\n",
        "              (\"Homo sapiens\", \"Homo floresiensis\"),\n",
        "              (\"Homo sapiens\", \"Homo luzonensis\")]\n",
        "\n",
        "# Añadimos las aristas al grafo\n",
        "G.add_edges_from(relaciones)\n",
        "\n",
        "# Dibujamos el grafo en forma de árbol con una imagen más pequeña\n",
        "plt.figure(figsize=(12, 6))  # Tamaño de la figura ajustado\n",
        "pos = nx.nx_pydot.graphviz_layout(G, prog=\"dot\")\n",
        "nx.draw(G, pos, with_labels=True, node_size=1000, node_color=\"lightblue\", font_size=10, font_weight=\"bold\", arrowsize=10)\n",
        "plt.title(\"Evolución del Ser Humano (Árbol)\")\n",
        "plt.show()\n"
      ],
      "metadata": {
