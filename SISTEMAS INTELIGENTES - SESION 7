    {
      "cell_type": "code",
      "source": [
        "import itertools\n",
        "import matplotlib.pyplot as plt\n",
        "import networkx as nx\n",
        "\n",
        "def es_primo(n):\n",
        "    if n <= 1:\n",
        "        return False\n",
        "    for i in range(2, int(n ** 0.5) + 1):\n",
        "        if n % i == 0:\n",
        "            return False\n",
        "    return True\n",
        "\n",
        "def encontrar_numeros_primos(maximo):\n",
        "    primos = []\n",
        "    for i in range(2, maximo):\n",
        "        if es_primo(i):\n",
        "            primos.append(i)\n",
        "    return primos\n",
        "\n",
        "def encontrar_conjetura_goldbach(numero):\n",
        "    primos = encontrar_numeros_primos(numero)\n",
        "    combinaciones = itertools.combinations(primos, 3)\n",
        "    soluciones = []\n",
        "    for combinacion in combinaciones:\n",
        "        if sum(combinacion) == numero:\n",
        "            soluciones.append(combinacion)\n",
        "    return soluciones\n",
        "\n",
        "def graficar_numeros_primos(numero, soluciones):\n",
        "    primos = encontrar_numeros_primos(numero)\n",
        "    G = nx.DiGraph()\n",
        "    G.add_nodes_from(primos)\n",
        "    plt.figure(figsize=(10, 8))\n",
        "    pos = nx.spring_layout(G, k=0.15)  # Ajustar el parámetro k para reducir la separación entre nodos\n",
        "    nx.draw(G, pos, node_color='skyblue', node_size=450, with_labels=True, font_size=10)\n",
        "    colores = ['red', 'green', 'blue', 'orange', 'purple', 'brown', 'pink', 'gray', 'cyan', 'magenta']\n",
        "    for i, solucion in enumerate(soluciones):\n",
        "        color = colores[i % len(colores)]\n",
        "        for i in range(len(solucion)):\n",
        "            for j in range(i+1, len(solucion)):\n",
        "                G.add_edge(solucion[i], solucion[j], color=color, arrows=True)\n",
        "    edges = G.edges()\n",
        "    colors = [G[u][v]['color'] for u,v in edges]\n",
        "    nx.draw_networkx_edges(G, pos, edgelist=edges, edge_color=colors, arrows=True, width=2)  # Ajustar el ancho de las flechas\n",
        "    plt.title('Números Primos')\n",
        "    plt.show()\n",
        "\n",
        "numero = int(input(\"Ingrese un número: \"))\n",
        "soluciones = encontrar_conjetura_goldbach(numero)\n",
        "if soluciones:\n",
        "    print(f\"La conjetura de Goldbach se puede aplicar {len(soluciones)} veces para {numero}.\")\n",
        "    for i, solucion in enumerate(soluciones):\n",
        "        print(f\"Solución {i+1}: {solucion} color {['red', 'green', 'blue', 'orange', 'purple', 'brown', 'pink', 'gray', 'cyan', 'magenta'][i % 10]}\")\n",
        "else:\n",
        "    print(f\"La conjetura de Goldbach no se puede aplicar para {numero}.\")\n",
        "\n",
        "graficar_numeros_primos(numero, soluciones)\n"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
          "height": 946
