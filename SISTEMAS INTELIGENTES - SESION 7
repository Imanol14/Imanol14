    {
      "cell_type": "code",
      "source": [
        "import networkx as nx\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "def construir_arbol_composicion(elementos, composiciones):\n",
        "    G = nx.DiGraph()\n",
        "    for i, composicion in enumerate(composiciones):\n",
        "        for elemento in composicion:\n",
        "            G.add_edge(elementos[i], elemento)\n",
        "    return G\n",
        "\n",
        "def dibujar_arbol_composicion(G):\n",
        "    plt.figure(figsize=(10, 8))\n",
        "    pos = nx.spring_layout(G)\n",
        "    nx.draw(G, pos, with_labels=True, node_color='lightblue', node_size=1500, font_size=12)\n",
        "    plt.title('Composición de elementos químicos')\n",
        "    plt.show()\n",
        "\n",
        "elementos = ['Agua', 'Hidrógeno', 'Oxígeno', 'Dióxido de carbono']\n",
        "composiciones = [['Hidrógeno', 'Oxígeno'], ['Hidrógeno'], ['Oxígeno'], ['Hidrógeno', 'Oxígeno']]\n",
        "\n",
        "G = construir_arbol_composicion(elementos, composiciones)\n",
        "dibujar_arbol_composicion(G)\n"
      ],
      "metadata": {
